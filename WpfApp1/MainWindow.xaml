<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="400">
    <Grid>
        <StackPanel HorizontalAlignment="Center" Margin="10">
            <Label FontSize="25" FontWeight="DemiBold">Wzory aplikacji desktopowych</Label>
            <Button Margin="20" Content="Styczeń 2023" Height="40" FontSize="20" Name="sty23" Click="sty23_Click"/>
            <Button Margin="20" Content="Styczeń 2024" Height="40" FontSize="20" Name="sty24" Click="sty24_Click"/>
            <Button Margin="20" Content="Czerwiec 2023" Height="40" FontSize="20" Name="cze23" Click="cze23_Click"/>
            <Button Margin="20" Content="Egzamin" Height="40" FontSize="20"/>
        </StackPanel>
    </Grid>
</Window>
